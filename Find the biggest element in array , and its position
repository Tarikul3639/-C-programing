#include<stdio.h>
int main()
{
    int arry[100];
    int max,i,n,pssition,j;
    scanf("%d",&n);
    for(i=0; i<n; i++)
    {
        scanf("%d",&arry[i]);
    }
    max=arry[0];
    for(i=0; i<n; i++)
    {
        if(max<=arry[i])
        {
            max=arry[i];

            pssition=i+1;
        }




    }
    //printf("%d\n",max);
    printf("%d\n",pssition);

    return 0;
}
