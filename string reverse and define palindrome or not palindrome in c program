#include<stdio.h>
#include<string.h>
int main()
{
    char str[100]="ABA";
    char str1[100];
    int i,j,k,d,revers;
    for(i=0;str[i]!='\0';i++)
    {

    }
    printf("%d\n",i);

    for(j=0,k=i-1;k>=0; k--,j++)
    {
        str1[j]=str[k];
    }
    str1[j]='\0';
    printf("%s\n",str1);



     d=strcmp(str,str1);



  if(d==0)
  
    printf("palindrom");
  
  else
  
    printf("not palindrom");
  
  printf("\n");
