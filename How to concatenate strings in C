
#include <stdio.h>
#include <string.h>
int main()
{
    char s1[100];
    char s2[100];
    char ans[100];
    int i;
    scanf("%s", s1);
    for (i = 0; s1[i] != '\0'; i++)
    {
        ans[i] = s1[i];
    }

    int c = 0;
    for (i = 0; s1[i] != '\0'; i++)
    {
        c++;
    }
    printf("Lenth for str1=%d\n", c);

    int j;
    scanf("%s", s2);
    for (j = 0; s1[j] != '\0'; j++)
    {
        ans[i + j] = s2[j];
    }
    ans[i + j] = '\0';
    printf("%s", ans);
    printf("\n");
}
