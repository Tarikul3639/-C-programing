#include <stdio.h>
int main()
{
    char str[100];
    char ans[100];
    int i, j, k;
    scanf("%s", str);
    int c = 0;
    for (i = 0; str[i] != '\0'; i++)
    {
        c++;
    }

    for (j = 0, i = c - 1; i>=0; i--, j++)
        
    {
        ans[j] = str[i];
    }

    printf("%s", ans);
}
